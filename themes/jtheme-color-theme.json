{
  "name": "JTheme",
  "type": "dark",
  "colors": {
    "editor.background": "#181A20CC",
    "editor.foreground": "#e3e8f7",
    "editor.selectionBackground": "#526de644",
    "editor.selectionHighlightBackground": "#3a5bff22",
    "editor.lineHighlightBorder": "#ffffff22",
    "editorCursor.foreground": "#ffe066",
    "editorBracketMatch.background": "#3a5bff22",
    "editorBracketMatch.border": "#7fd7ff",
    "editorLineNumber.foreground": "#b0b8d0aa",
    "editorLineNumber.activeForeground": "#ffe066",
    "minimap.background": "#181A20cc",
    "statusBar.background": "#181A20cc",
    "statusBar.foreground": "#e3e8f7",
    "sideBar.background": "#181A20b3",
    "sideBar.foreground": "#e3e8f7",
    "panel.background": "#181A20b3",
    "panel.border": "#ffffff22",
    "activityBar.background": "#232a3a99",
    "activityBar.foreground": "#e3e8f7",
    "activityBar.activeBorder": "#7fd7ff",
    "tab.activeBackground": "#232a3acc",
    "tab.inactiveBackground": "#181A2099",
    "tab.activeForeground": "#e3e8f7",
    "tab.inactiveForeground": "#b0b8d0aa",
    "tab.border": "#ffffff22",
    "dropdown.background": "#202430",
    "dropdown.border": "#7fd7ff",
    "scrollbarSlider.background": "#7fd7ff33",
    "scrollbarSlider.hoverBackground": "#7fd7ff55",
    "scrollbarSlider.activeBackground": "#7fd7ff77",
    "terminal.foreground": "#b3c0df",
    "terminalCursor.background": "#A89984",
    "terminalCursor.foreground": "#7b69bb",
    "terminal.ansiBlack": "#1D2021",
    "terminal.ansiBlue": "#0D6678",
    "terminal.ansiBrightBlack": "#665C54",
    "terminal.ansiBrightBlue": "#0D6678",
    "terminal.ansiBrightCyan": "#8BA59B",
    "terminal.ansiBrightGreen": "#95C085",
    "terminal.ansiBrightMagenta": "#8F4673",
    "terminal.ansiBrightRed": "#FB543F",
    "terminal.ansiBrightWhite": "#FDF4C1",
    "terminal.ansiBrightYellow": "#fbfdfd",
    "terminal.ansiCyan": "#8BA59B",
    "terminal.ansiGreen": "#95C085",
    "terminal.ansiMagenta": "#8F4673",
    "terminal.ansiRed": "#FB543F",
    "terminal.ansiWhite": "#A89984",
    "terminal.ansiYellow": "#FAC03B",
    "editor.lineHighlightBackground": "#1073cf2d",
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7fffd4cc",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#b2e1ff"
      }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#ffe066",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#7fd7ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#b2e1ff"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffb2e1"
      }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#e1b2ff"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffe066"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffb2e1"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#7fd7ff"
      }
    },
    {
      "scope": "entity.other",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#b2b8d4"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66a566"
      }
    },
    {
      "scope": "storage.type.function.arrow.js",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ffcb6b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#c3e88d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "scope": "punctuation.definition.string, punctuation.definition.variable",
      "settings": {
        "foreground": "#89ddff"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#c3e88d"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ffffff",
        "background": "#ff5370"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffcb6b"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f78c6c"
      }
    }
  ]
}
